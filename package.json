{
  "name": "webdismay",
  "version": "0.1.0",
  "description": "disturbingly open browser-side library for webdis ",
  "main": "build/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -rf ./build ./doc ; mkdir ./build",
    "build": "babel build src/index.js -o build/index.js",
    "doc": "esdoc -c esdoc.json",
    "lint": "eslint src/index.js",
    "lint-test": "eslint test/index.js",
    "test": "mocha --compilers js:babel-core/register --reporter spec --slow 50 --timeout 60000",
    "cover": "istanbul cover node_modules/.bin/_mocha -- -u exports --compilers js:babel-register --timeout 60000",
    "go": "npm run clean && npm run lint && npm run lint-test && npm run test && npm run build"
  },
  "author": "drpaulbrewer@eaftc.com",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.9",
    "esdoc": "^0.4.7",
    "eslint": "^3.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.5.3",
    "should": "^8.3.1"
  },
  "dependencies": {},
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drpaulbrewer/webdismay.git"
  },
  "keywords": [
    "webdis",
    "database",
    "browser-side"
  ],
  "bugs": {
    "url": "https://github.com/drpaulbrewer/webdismay/issues"
  },
  "homepage": "https://github.com/drpaulbrewer/webdismay#readme"
}
